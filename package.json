{
  "name": "apigee-stilo",
  "version": "0.1.4",
  "description": "Apigee Styles library to be used by SPAs with Webpack and Typescript or as a raw CSS dependency",
  "main": "index.js",
  "scripts": {
    "clean": "node ./scripts/clean.js",
    "copy:build": "cp -r build/ docs/css/",
    "build:wp": "webpack --config ./scripts/webpack.build.conf.js -p",
    "build": "npm run clean && npm run build:wp && npm run copy:build",
    "test": "karma start ./scripts/karma.conf.js",
    "start": "node ./scripts/server.js",
    "deploy": "node ./scripts/deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apigee-internal/apigee-stilo.git"
  },
  "keywords": [
    "Apigee",
    "CSS",
    "Styles"
  ],
  "author": "Oscar Ponce",
  "license": "Apache-2.0",
  "devDependencies": {
    "apigee-icons": "git+https://github.com/apigee-internal/apigee-icons.git#v1.0.2",
    "autoprefixer": "^6.4.1",
    "bootstrap": "3.3.7",
    "css-loader": "0.25.0",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "http-proxy": "^1.15.1",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "lato-font": "^2.0.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "postcss-loader": "^0.13.0",
    "ts-loader": "0.8.2",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.3",
    "typings": "1.3.3",
    "webpack": "1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "lodash": "^4.16.2"
  }
}
